    "styled-components": "^5.2.1",
    "tailwindcss": "2.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@testing-library/react": "^11.2.5",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.27",
