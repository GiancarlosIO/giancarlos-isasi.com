    "eslint-plugin-testing-library": "^3.9.0",
    "favicons": "^6.2.0",
    "husky": "4",
    "jest": "^26.6.3",
    "jest-styled-components": "^7.0.3",
    "lint-staged": "^10.5.4",
    "prettier": "2.3.1",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "4.3.4",
    "typescript-plugin-styled-components": "^1.4.4"
